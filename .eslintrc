{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "max-len": "off",
        "eol-last": "off",
        "require-jsdoc": "off"
    }
}